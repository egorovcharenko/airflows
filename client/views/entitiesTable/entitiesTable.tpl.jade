#content.page-container.container
  #title_row.row
    .col.s12
      #page_menu.pull-right
  .row
    +entitiesButtons
  .row
    table.table.table-striped.table-hover
      thead
        tr
          th Создано
          th Обновлено
          th Автор
          th Статус
          th Текущие шаги
          each getFields
            th #{name}
      tbody
        each this.entIns
          tr
            td {{prettifyDate createdAt}}
            td {{prettifyDate modifiedAt}}
            td #{getUser.profile.name}
            td #{state}
            td {{{currentTask}}}
            each getFieldValues
              td #{value}
